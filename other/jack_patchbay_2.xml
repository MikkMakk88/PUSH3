<!DOCTYPE patchbay>
<patchbay version="0.5.0" name="jack_patchbay_2">
 <output-sockets>
  <socket client="system" type="jack-audio" name="system 1" exclusive="off">
   <plug>capture_1</plug>
   <plug>capture_2</plug>
  </socket>
  <socket client="PulseAudio JACK Sink" type="jack-audio" name="PulseAudio JACK Sink 1" exclusive="off">
   <plug>front-left</plug>
   <plug>front-right</plug>
  </socket>
  <socket client="pure_data" type="jack-audio" name="pd_out" exclusive="on">
   <plug>output0</plug>
   <plug>output1</plug>
   <plug>output2</plug>
   <plug>output3</plug>
  </socket>
  <socket client="Bitwig Studio" type="jack-audio" name="Bitwig Studio 1" exclusive="off">
   <plug>Speakers-L</plug>
   <plug>Speakers-R</plug>
  </socket>
  <socket client="vlc_[0-9]+" type="jack-audio" name="vlc_[0-9]+ 1" exclusive="on">
   <plug>vlc_[0-9]+_out_1</plug>
   <plug>vlc_[0-9]+_out_2</plug>
  </socket>
 </output-sockets>
 <input-sockets>
  <socket client="system" type="jack-audio" name="system_in" exclusive="on">
   <plug>playback_1</plug>
   <plug>playback_2</plug>
  </socket>
  <socket client="PulseAudio JACK Source" type="jack-audio" name="PulseAudio JACK Source 1" exclusive="off">
   <plug>front-left</plug>
   <plug>front-right</plug>
  </socket>
  <socket client="pure_data" type="jack-audio" name="pd_in" exclusive="on">
   <plug>input0</plug>
   <plug>input1</plug>
   <plug>input2</plug>
   <plug>input3</plug>
  </socket>
  <socket client="Bitwig Studio" type="jack-audio" name="Bitwig Studio 1" exclusive="off">
   <plug>Stereo In-L</plug>
   <plug>Stereo In-R</plug>
  </socket>
 </input-sockets>
 <slots/>
 <cables>
  <cable type="jack-audio" output="PulseAudio JACK Sink 1" input="system_in"/>
  <cable type="jack-audio" output="pd_out" input="PulseAudio JACK Source 1"/>
  <cable type="jack-audio" output="vlc_[0-9]+ 1" input="pd_in"/>
 </cables>
</patchbay>

